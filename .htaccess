RewriteEngine on

# Redirect to another domain: vuugle.co.
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} !^(www\.)?vuugle\.co$ [NC]
RewriteRule .* http://vuugle.co%{REQUEST_URI} [R=301,L]
# Same for HTTPS:
RewriteCond %{HTTPS} on
RewriteCond %{HTTP_HOST} !^(www\.)?vuugle\.co$ [NC]
RewriteRule .* https://vuugle.co%{REQUEST_URI} [R=301,L]
